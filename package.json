{
  "name": "oxygenide",
  "productName": "OxygenIDE",
  "version": "1.5.0",
  "description": "IDE for Oxygen automation framework.",
  "main": "./app/main/main.prod.js",
  "build": {
    "productName": "OxygenIDE",
    "appId": "oxygen.ide",
    "copyright": "Copyright Â© year ",
    "mac": {
      "target": "pkg",
      "category": "public.app-category.developer-tools"
    },
    "pkg": {
      "scripts": "build/pkg-scripts"
    },
    "files": [
      "./build/**/*",
      "./app/main/main.prod.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "asar": false
  },
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "browserslist": "electron 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxygenhq/oxygen-ide.git"
  },
  "author": {
    "name": "cloudbeat.io",
    "email": "ndimer@cloudbeat.io"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen-ide/issues"
  },
  "homepage": "https://github.com/oxygenhq/oxygen-ide",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "electron": "<rootDir>/test/__mocks__/electron.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(antd)/)"
    ],
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ],
    "testURL": "file:/",
    "verbose": true,
    "useStderr": true
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.6",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.10",
    "electron": "^3.1.8",
    "electron-builder": "^20.40.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.80.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "grunt": "^1.0.3",
    "grunt-chmod": "^1.1.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mini-css-extract-plugin": "^0.4.3",
    "modclean": "^2.1.2",
    "node-sass": "^4.7.2",
    "postcss-discard-font-face": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "recursive-copy": "^2.0.9",
    "redux-logger": "^3.0.6",
    "repeat-string": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "chokidar": "2.1.5",
    "monaco-editor": "0.14.3",
    "oxygen-cli": "0.42.8",
    "xml2json": "^0.11.2",
    "antd": "^3.4.5",
    "appium-adb": "^7.6.5",
    "clean-stack": "^2.1.0",
    "detect-port": "^1.3.0",
    "devtron": "^1.4.0",
    "electron-debug": "^2.2.0",
    "electron-settings": "^3.2.0",
    "firacode": "^1.206.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^7.0.1",
    "junk": "^3.1.0",
    "lodash": "^4.17.11",
    "mixpanel": "^0.10.2",
    "moment": "^2.24.0",
    "os-locale": "^3.1.0",
    "perfect-scrollbar": "^1.4.0",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.8.3",
    "react-icons": "^2.2.7",
    "react-redux": "^5.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "source-map-support": "^0.5.11",
    "uniqid": "^5.0.3",
    "uuid": "^3.3.2",
    "winston": "^2.4.4"
  },
  "optionalDependencies": {
    "appdmg": "^0.5.2"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x"
  }
}
